;simd_pmadd
;

format ELF executable 3
entry start

segment readable writeable


X dq 0x5555AAAA6666BBBB;

Y dq 0x3333111122225555;

segment readable executable

start:

pxor mm0,mm0
pxor mm1,mm1

movq mm0, [X]
movq mm1, [X]
movq mm2, [Y]
paddw mm0, [Y] ; mm0 = 8888 BBBB 8888 FFFF


; now exit
exit:
	mov eax, 1
	xor	ebx,ebx         ; exit code == 0
	int 0x80   


